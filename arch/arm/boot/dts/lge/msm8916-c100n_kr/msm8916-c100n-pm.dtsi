		rt,r1_gain_hcap = < 0 100 0 365 0 100>;
		rt,r2_gain_hcap = < 1 250 1 50 1 250>;
		rt,r3_gain_hcap = < 1 650 0 0 1 650>;
		rt,r4_gain_hcap = < 0 0 0 0 0 0>;
		rt,r1_gain_lcap = < 0 100 0 365 0 100>;
