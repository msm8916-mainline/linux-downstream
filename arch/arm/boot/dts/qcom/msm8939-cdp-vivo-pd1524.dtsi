	led {
		compatible = "vivo,led";
		vivo,name = "green";
		vivo,led-share-pwm;
		led,pwm_period=<2000000>;
		led,pwm_duty=<200000>;
	};
	vivo-hifi-codec {
		compatible = "vivo,vivo-codec-pd1524";
	};
	sensorcfg {
		compatible = "vivo,sensor-config";
		vivo,product-model = "PD1524";
		ps-para0 {
			base-val = <200>;
			low-thres = <0>;
			high-thres = <800>;
			step-num = <3>;
			step0 = <  0 300 230 70>;
			step1 = <300 500 220 65>;
			step2 = <500 800 210 60>;
		};
		
		als-para0 {
			gain = <164>;
			low-thres = <50>;
			high-thres = <1500>;
		};
	};
	switch_gsensor {
		  compatible = "vivo-gsensor";
		  sensor-name = "gsensor";
		/*  sensor,gpio-irq = <&msm_gpio 21 0x00>;  */
	};
	i2c@78b6000 { /* BLSP1 QUP2 */
		TMD2771@39 {
			compatible = "TMD2771";
			vdd-supply = <&pm8916_l17>;
			vddio-supply = <&pm8916_l6>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <113 0x00>;
			pinctrl-names = "default";
			pinctrl-0 = <&apds99xx_default>;
			reg = <0x39>;
			irq-gpio = <&msm_gpio 113 0x00>;
		};
		lis3dh_acc:lis3dh_acc@18 {
			compatible = "lis3dh_acc";
			reg = <0x18>;
			vdd-supply = <&pm8916_l17>;
			vddio-supply = <&pm8916_l5>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <114 0x00>;
			st,min-interval = <10>;
			st,init-interval = <200>;
			st,axis-map-x = <0>;
			st,axis-map-y = <1>;
			st,axis-map-z = <2>;
			st,g-range = <2>;
			st,negate-x = <1>;
			st,negate-y = <0>;
			st,negate-z = <1>;
			sensor,gpio-irq = <&msm_gpio 114 0x00>;			
		};
		akm8963@0C {
			compatible = "akm,akm8963";
			reg = <0x0C>;
			ak,layout =<3>;
			ak,auto-report;
			vdd-supply = <&pm8916_l17>;
			vio-supply = <&pm8916_l5>;
			iic-address=<0x0C>;
		};
		yas533@2E {
			compatible = "yas533_mag";
			reg = <0x2E>;
			ak,layout =<3>;
			vdd-supply = <&pm8916_l17>;
			vio-supply = <&pm8916_l5>;
		};
	};
	
	fp_id:fp_id {
		compatible = "fp-id";
		status = "ok";
		vivo,project-name = "PD1524";
		vcc_fpc-supply  = <&pm8916_l16>;
		fp_id,gpios = <&msm_gpio 108 0>;
		pinctrl-names = "fp_gpio_pull_up",
						"fp_gpio_pull_down";
		pinctrl-0 = <&fp_id_gpio_up>;
		pinctrl-1 = <&fp_id_gpio_down>;
	};
	
	i2c@78ba000 { /* BLSP1 QUP6 */
		status = "disabled";
	};
	// add by gongyulong for fingerprint
	spi@78ba000 {
		status = "ok";
		vcc_spi-supply  = <&pm8916_l16>;
		
	    fpc_fpc1022@0 {
	    compatible = "fpc,fpc1022";
		spi-max-frequency = <4800000>;
	    reg = <0>;
	    input-device-name = "fpc1022";
	    interrupt-parent = <&msm_gpio>;
	    interrupts = <115 0>;

	    fpc,use_fpc2050 = <1>;
		fpc,enable-on-boot;
		fpc,enable-wakeup;
		fpc,event-type = <4>;
		fpc,event-code = <4>;
		spi-qup-id = <6>;
		vcc_spi-supply  = <&pm8916_l16>;
		vdd_ana-supply  = <&pm8916_l16>;
		vdd_io-supply   = <&pm8916_l16>;

	    fpc,gpio_reset = <&msm_gpio 74 0x00>;
		fpc,gpio_irq = <&msm_gpio 115 0x00>;

		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp1_ahb_clk>,
				<&clock_gcc clk_gcc_blsp1_qup6_spi_apps_clk>;

		pinctrl-names = "fpc1020_irq_active",
				"fpc1020_reset_active",
				"fpc1020_reset_reset";
		pinctrl-0 = <&fpc_irq_active>;
		pinctrl-1 = <&fpc_reset_active>;
		pinctrl-2 = <&fpc_reset_reset>;
		};
		
		goodix_gf3208@1 {
        compatible = "goodix,fingerprint";
        spi-max-frequency = <4800000>;
        reg = <1>;
        interrupt-parent = <&msm_gpio>;
        interrupts = <115 0>;

        vcc_spi-supply  = <&pm8916_l17>;

        goodix,gpio_reset = <&msm_gpio 74 0x00>;
        goodix,gpio_irq = <&msm_gpio 115 0x00>;

        clock-names = "iface_clk", "core_clk";
        clocks = <&clock_gcc clk_gcc_blsp1_ahb_clk>,
                <&clock_gcc clk_gcc_blsp1_qup6_spi_apps_clk>;
        };		
	};

	qseecom@85100000 {
		compatible = "qcom,qseecom";
		reg = <0x85100000 0x1400000>;
		reg-names = "secapp-region";
		qcom,disk-encrypt-pipe-pair = <2>;
		qcom,hlos-ce-hw-instance = <0>;
		qcom,qsee-ce-hw-instance = <0>;
		qcom,msm-bus,name = "qseecom-noc";
		qcom,msm-bus,num-cases = <4>;
		qcom,msm-bus,num-paths = <1>;
		qcom,support-bus-scaling;
		qcom,support-fde;
		qcom,msm-bus,vectors-KBps =
				<55 512 0 0>,
				<55 512 0 0>,
				<55 512 120000 1200000>,
				<55 512 393600 3936000>;
		clocks = <&clock_gcc clk_crypto_clk_src>,
			 <&clock_gcc clk_gcc_crypto_clk>,
			 <&clock_gcc clk_gcc_crypto_ahb_clk>,
			 <&clock_gcc clk_gcc_crypto_axi_clk>;
		clock-names = "core_clk_src", "core_clk",
				"iface_clk", "bus_clk";
		status = "ok";
		qcom,ce-opp-freq = <100000000>;
	}; 
};

&apc_vreg_corner {
		vdd-apc-supply = <&fan53555>;
		 qcom,cpr-down-threshold = <8>;
		 qcom,cpr-step-quotient = <26>;
		 qcom,cpr-apc-volt-step = <12500>;
		 qcom,cpr-up-threshold = <2>; 
};
&restart {
	qcom,download_mode-disable;
};
&mss {
	qcom,subsystem-restart-level;
};
&venus {
	qcom,subsystem-restart-level;
};
&pronto {
	qcom,subsystem-restart-level;
};

